{
  "version": "1.0.0",
    "description": "Loggr.Extensions.Logging console application",
    "authors": [ "mbishopim3" ],
    "packOptions": {
        "tags": [ "loggr" ],
        "projectUrl": "https://github.com/imobile3/Loggr.Extensions.Logging",
        "iconUrl": "http://static.loggr.net/1/images/nuget/logo.png"
    },

    "dependencies": {
        "Loggr.Extensions.Logging": "1.0.0"
    },

    "frameworks": {
        "netframework451": {
            "imports": [ "dnx451" ],
            "frameworkAssemblies": {
                "System.Runtime": ""
            }
        },
        "netcoreapp1.0": {
            "imports": [ "dnxcore50" ],
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0"
                }
            }
        }
    },

    "runtimeOptions": {
        "gcServer": false,
        "gcConcurrent": true
    },

    "buildOptions": {
        "emitEntryPoint": true
    }
}
